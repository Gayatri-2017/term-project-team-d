{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Resources": {      
      "tableUsers": {
        "Type": "AWS::DynamoDB::Table",
        "Properties": {
          "TableName": "user-team-d",
          "AttributeDefinitions": [
            {
              "AttributeName": "user_id",
              "AttributeType": "S"
            }
          ],
          "KeySchema": [
            {
              "AttributeName": "user_id",
              "KeyType": "HASH"
            }
          ],
          "ProvisionedThroughput": {
            "ReadCapacityUnits": "5",
            "WriteCapacityUnits": "5"
          }
        }
      },
